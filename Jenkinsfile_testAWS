pipeline {
    agent any

    stages {
        stage('testAWS') {
            steps {
                withAWS(credentials: 'aws_jr_william', region: 'ap-southeast-2') {
                    // some block
                    sh 'aws --version'
                    sh 'aws sts get-caller-identity'
                    sh 'aws lambda list-functions --max-items 3'
                }
            }
        }
    }
}
